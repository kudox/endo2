(function(){window.getCookie=function(cookieName){var cookie,cookies,decodedCookies,name,_i,_len;name=cookieName+"=";decodedCookies=decodeURIComponent(document.cookie);cookies=decodedCookies.split(';');for(_i=0,_len=cookies.length;_i<_len;_i++){cookie=cookies[_i];while(cookie.charAt(0)===' ')cookie=cookie.substring(1);if(cookie.indexOf(name)===0)return cookie.substring(name.length,cookie.length)};return""};window.setCookie=function(cookieName,cookieValue,daysTilExpiration){var date,newCookie;if(daysTilExpiration==null)daysTilExpiration=0;newCookie=cookieName+"="+cookieValue+";";if(daysTilExpiration>0){date=new Date();date.setTime(date.getTime()+(daysTilExpiration*24*60*60*1e3));newCookie+="expires="+date.toUTCString()+";"};document.cookie=newCookie};window.validateDate=function(date){var pattern;pattern=new RegExp(/^([0-9]{2})\/([0-9]{2})\/([0-9]{4})$/);return pattern.test(date)};window.validatePhone=function(tel){var error,stripped;error="";stripped=tel.replace(/[\.\(\)\@\!\-\#\$\%\^\&\*\=\_\|\ ]/g,'');if(tel===""){error="Required\n"}else if(isNaN(parseInt(stripped))){error="Invalid Phone format\n"}else if(!(stripped.length===10)&&!(stripped.length===11))error="Invalid Phone format\n";return error};window.isValidEmailAddress=function(emailAddress){var pattern;pattern=new RegExp(/^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);return pattern.test(emailAddress)};window.timeInMinutes=function(time){var hours,meridian,minutes,totalMinutes;meridian=time.split(' ')[1];hours=parseInt(time.split(':')[0]);minutes=parseInt(time.split(':')[1]);totalMinutes=0;if(meridian==='am'){if(hours===12)hours=0}else if(hours!==12)hours+=12;return(hours*60)+minutes};window.getUrlParameters=function(queryString){var multiparam,parameters;if(queryString==null)queryString=window.location.search;if(!(queryString!=null)||queryString===null)queryString="";queryString=queryString.replace("?","");multiparam=/\[\]/;parameters={};if(queryString&&queryString.length>0)$.each(queryString.split('&'),function(c,query){var key,value,_ref;_ref=query.split('='),key=_ref[0],value=_ref[1];key=decodeURIComponent(key);value=decodeURIComponent(value).replace(/\+/g," ");if(key.match(multiparam)){key=key.replace(multiparam,'');if(parameters[key]==null)parameters[key]=[]};if(parameters[key]instanceof Array){return parameters[key].push(value)}else return parameters[key]=value});return parameters};window.getUrlParameter=function(name,backup){var param,params;if(backup==null)backup=null;params=getUrlParameters();return param=_.has(params,name)?params[name]:backup};window.clone=function(obj){var flags,key,newInstance;if(!(obj!=null)||typeof obj!=='object')return obj;if(obj instanceof Date)return new Date(obj.getTime());if(obj instanceof RegExp){flags='';if(obj.global!=null)flags+='g';if(obj.ignoreCase!=null)flags+='i';if(obj.multiline!=null)flags+='m';if(obj.sticky!=null)flags+='y';return new RegExp(obj.source,flags)};newInstance=new obj.constructor();for(key in obj)newInstance[key]=clone(obj[key]);return newInstance};window.isIE9OrBelow=function(){return/MSIE\s/.test(window.navigator.userAgent)&&parseFloat(window.navigator.appVersion.split("MSIE")[1])<10};window.goToByScroll=function(jquerySelector){var headerOffset;headerOffset=0;if($('header').css('position')==='fixed')headerOffset=$('header').height();return $('html,body').animate({scrollTop:$(jquerySelector).offset().top-headerOffset},'slow')};(function($){$.fn.nofollow=function(options){return this.each(function(){var $this,elementData,i,newElementHtml;$this=$(this);elementData=$this.data('element');newElementHtml='';i=0;while(i<elementData.length){newElementHtml+=String.fromCharCode(parseInt(elementData.substring(i,i+2),16));i+=2};$this.replaceWith(newElementHtml)})}})(jQuery);window.brightenColor=function(code,brightness){var b,g,num,r;num=parseInt(code,16);r=(num>>16)+brightness;if(r>255){r=255}else if(r<0)r=0;b=((num>>8)&0x00FF)+brightness;if(b>255){b=255}else if(b<0)b=0;g=(num&0x0000FF)+brightness;if(g>255){g=255}else if(g<0)g=0;return(g|(b<<8)|(r<<16)).toString(16)};window.rgb2hex=function(rgb){var hex;if(rgb.search("rgb")===-1){return rgb}else{rgb=rgb.match(/^rgba?\((\d+),\s*(\d+),\s*(\d+)(?:,\s*(\d+))?\)$/);hex=function(x){return("0"+parseInt(x).toString(16)).slice(-2)};return hex(rgb[1])+hex(rgb[2])+hex(rgb[3])}}}).call(this);;
(function(){var __bind=function(fn,me){return function(){return fn.apply(me,arguments)}};this.patientpop=window.patientpop||{};_.extend(this.patientpop,{reputation:{}});patientpop.reputation.Modal=(function(){function Modal(modalDiv,widgetOrigin,siteName){this.redirectTo=__bind(this.redirectTo,this);this.scrollToTop=__bind(this.scrollToTop,this);this.resizeIFrameHeight=__bind(this.resizeIFrameHeight,this);this.closeModal=__bind(this.closeModal,this);this.handleIncomingMessage=__bind(this.handleIncomingMessage,this);this.initialize=__bind(this.initialize,this);window.addEventListener('message',this.handleIncomingMessage,false);this.initialize(modalDiv,widgetOrigin,siteName);return};Modal.prototype.initialize=function(modalDiv,widgetOrigin,siteName){var iframeSrc,widgetHtml,_this=this;iframeSrc=(""+widgetOrigin+"/reputation/"+siteName)+window.location.search;widgetHtml="    <div class='modal' id='leave-review' tabindex='-1' role='dialog' aria-hidden='true' aria-labelledby='leaveReviewLabel'>      <div class='modal-dialog'>        <div class='modal-content'>          <button type='button' class='close' aria-hidden='true'>&times;</button>          <div class='modal-body'>            <iframe id='reputation-widget' scrolling='no' src=''></iframe>          </div>        </div>      </div>    </div>";modalDiv.innerHTML=widgetHtml;this.widgetOrigin=widgetOrigin;this.$reviewModal=$(modalDiv).find('.modal');this.$iframe=this.$reviewModal.find('iframe');this.$closeButton=this.$reviewModal.find('button.close');this.$reviewModal.find('button.close').click(this.closeModal);this.$reviewModal.on('show.bs.modal',function(){if(typeof ga!=="undefined"&&ga!==null)ga('send','event',{eventCategory:'button',eventAction:'click',eventLabel:'leave review'});return _this.$iframe.attr('src',iframeSrc)});this.$reviewModal.on('shown.bs.modal',function(){_this.$reviewModal.modal('handleUpdate');$('body').css('overflow','hidden');return $('body').css('position','fixed')});if(this.hasReputationQueryParams())return this.$reviewModal.modal('show')};Modal.prototype.hasReputationQueryParams=function(){return this.hasQueryParam('review')||this.hasQueryParam('location')||this.hasQueryParam('email')||this.hasQueryParam('recommend')||this.hasQueryParam('provider')};Modal.prototype.hasQueryParam=function(paramName){return window.getUrlParameter(paramName)!=null};Modal.prototype.handleIncomingMessage=function(messageEvent){var event;if(messageEvent.origin!==this.widgetOrigin){console.log('wrong origin');return};try{event=JSON.parse(messageEvent.data);switch(event.eventType){case'close':return this.closeModal();case'setHeight':return this.resizeIFrameHeight(event.height);case'scrollToTop':return this.scrollToTop();case'redirectTo':return this.redirectTo(event.url)}}catch(e){return false}};Modal.prototype.closeModal=function(){this.$reviewModal.modal('hide');return $('body').css({overflow:'auto',position:'relative'})};Modal.prototype.resizeIFrameHeight=function(height){this.$iframe.height(height);if(this.$reviewModal.find('.modal-backdrop').length)return this.$reviewModal.modal('handleUpdate')};Modal.prototype.scrollToTop=function(){return this.$reviewModal.scrollTop(0,0)};Modal.prototype.redirectTo=function(url){return window.location.replace(url)};return Modal})()}).call(this);;
(function(){pop.appointment={url:"/widgets/appointment/get/",pulled:false,id:null,setUp:function(id){this.id=id;return this},pull:function(async){if(async==null)async=true;if(!(this.id!=null)){alert('appointment id is missing');return};$.ajax({type:"POST",async:async,url:this.url+this.id,data:{_token:pop.csrf},success:function(data){if(data.status==='success'){_.each(data.content,function(value,key,object){return pop.appointment[key]=value});pop.appointment.pulled=true;return pop.appointment}else{alert((data.content!=null)&&data.content!==''?data.content:'Sorry there was an error retrieving the appointment. Please try again.');return null}},error:function(){alert('Sorry there was an error retrieving the appointment. Please try again.');return null}})},reset:function(){var pull,reset,setUp,url;pull=pop.appointment.pull;reset=pop.appointment.reset;setUp=pop.appointment.setUp;url=pop.appointment.url;pop.appointment={url:url,pulled:false,id:null,pull:pull,reset:reset,setUp:setUp}}}}).call(this);;
(function(){var root;root=typeof exports!=="undefined"&&exports!==null?exports:this;if(typeof pop!=="undefined"&&pop!==null)pop.providers={filterNewPatients:'',filterSpecialties:'',filterInsurance:'',filterGender:'',filterLangs:'',filterLocations:'',init:function(){return pop.providers.turnOn()},turnOn:function(){$('.provider-list-filters select').on('change',pop.providers.filter);$('.provider-list-filters input[type="checkbox"]').on('change',pop.providers.filter)},turnOff:function(){$('.provider-list-filters select').off('change');$('.provider-list-filters input[type="checkbox"]').off('change')},filter:function(){var filterName,filterVal;filterName=$(this).attr('id');if(filterName==='new_patients'){filterVal=$(this).prop('checked')?"1":""}else filterVal=$(this).val();switch(filterName){case'new_patients':pop.providers.filterNewPatients=filterVal;break;case'specialties':pop.providers.filterSpecialties=filterVal;break;case'insurance':pop.providers.filterInsurance=filterVal;break;case'gender':pop.providers.filterGender=filterVal;break;case'languages':pop.providers.filterLangs=filterVal;break;case'locations':pop.providers.filterLocations=filterVal};pop.providers.filterDoctors();return false},filterDoctors:function(){$('.provider-list-block').addClass('notfound');$('.provider-list-block').each(function(){var found;found=true;if(pop.providers.filterNewPatients!==''&&$(this).find('[data-newpatient="'+pop.providers.filterNewPatients+'"]').length===0)found=false;if(pop.providers.filterSpecialties!==''&&$(this).find('[data-spec="'+pop.providers.filterSpecialties+'"]').length===0)found=false;if(pop.providers.filterInsurance!==''&&$(this).find('[data-ins="'+pop.providers.filterInsurance+'"]').length===0)found=false;if(pop.providers.filterGender!==''&&$(this).find('[data-gender="'+pop.providers.filterGender+'"]').length===0)found=false;if(pop.providers.filterLangs!==''&&$(this).find('[data-lang="'+pop.providers.filterLangs+'"]').length===0)found=false;if(pop.providers.filterLocations!==''&&$(this).find('[data-loc="'+pop.providers.filterLocations+'"]').length===0)found=false;if(found)return $(this).removeClass('notfound')});$('.provider-list-block.notfound').hide('slow');$('.provider-list-block').not('.notfound').show('slow');if($('.provider-list-block.notfound').length===$('.provider-list-block').length){return $('.provider-list-block-noresults').show('slow')}else return $('.provider-list-block-noresults').hide()}}}).call(this);;
(function(){var root;root=typeof exports!=="undefined"&&exports!==null?exports:this;root.recaptchaCallback=function(){if($('#recaptcha1').length)grecaptcha.render('recaptcha1',{sitekey:recaptcha_public_key});if($('#recaptcha2').length)root.widgetIdRecaptcha=grecaptcha.render('recaptcha2',{sitekey:recaptcha_public_key})}}).call(this);;